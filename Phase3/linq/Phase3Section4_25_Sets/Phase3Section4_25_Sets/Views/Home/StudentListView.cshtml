@{
    ViewData["Title"] = "StudentListView";
}

<h2>StudentListView</h2>
<div class="container">
    <br />
    <h4>List Of Students</h4>
    <div class="row">
        <div class="col-sm-7">
            <div class="row">
                <div class="col-sm-3">Name/Marks</div>
                <div class="col-sm-3">Email</div>
                <div class="col-sm-3">Class</div>
                <div class="col-sm-3">Address </div>
            </div>
            <hr />
            @{
                List<Student> students = (List<Student>)ViewData["students"];
                foreach (Student st in students)
                {
                    <div class="row">
                        <div class="col-sm-3">@st.Name - @st.MarksPercent%</div>
                        <div class="col-sm-3">@st.Email</div>
                        <div class="col-sm-3">@st.Class</div>
                        <div class="col-sm-3">@st.Address </div>
                    </div>
                }
            }
            <div class="clearfix"></div><br />

            <h4>StudentList using Union of two lists</h4>
            <div class="row">
                <div class="col-sm-3">Name/Marks</div>
                <div class="col-sm-3">Email</div>
                <div class="col-sm-3">Class</div>
                <div class="col-sm-3">Address </div>
            </div>
            <hr />
            @{
                List<Student> union = (List<Student>)ViewData["union"];
                foreach (Student st in union)
                {
                    <div class="row">
                        <div class="col-sm-3">@st.Name - @st.MarksPercent%</div>
                        <div class="col-sm-3">@st.Email</div>
                        <div class="col-sm-3">@st.Class</div>
                        <div class="col-sm-3">@st.Address </div>
                    </div>
                }
            }
            <div class="clearfix"></div><br />

            <h4>StudentList using Intersections of two lists</h4>
            <div class="row">
                <div class="col-sm-3">Name/Marks</div>
                <div class="col-sm-3">Email</div>
                <div class="col-sm-3">Class</div>
                <div class="col-sm-3">Address </div>
            </div>
            <hr />
            @{
                List<Student> intersect = (List<Student>)ViewData["intersect"];
                foreach (Student st in intersect)
                {
                    <div class="row">
                        <div class="col-sm-3">@st.Name - @st.MarksPercent%</div>
                        <div class="col-sm-3">@st.Email</div>
                        <div class="col-sm-3">@st.Class</div>
                        <div class="col-sm-3">@st.Address </div>
                    </div>
                }
            }
            <div class="clearfix"></div><br />

            <h4>StudentList with Distinct Marks</h4>
            <div class="row">
                <div class="col-sm-3">Marks</div>
            </div>
            <hr />
            @{
                IEnumerable<dynamic> distinct = (IEnumerable<dynamic>)ViewData["distinct"];

                foreach (dynamic st in distinct)
                {

                    <div class="row">
                        <div class="col-sm-3"> @st.GetType().GetProperty("MarksPercent").GetValue(st, null)</div>
                    </div>
                }

            }
            <div class="clearfix"></div><br />

            <h4>StudentList using Except with two lists</h4>
            <div class="row">
                <div class="col-sm-3">Name/Marks</div>
                <div class="col-sm-3">Email</div>
                <div class="col-sm-3">Class</div>
                <div class="col-sm-3">Address </div>
            </div>
            <hr />
            @{
                List<Student> except = (List<Student>)ViewData["except"];
                foreach (Student st in except)
                {
                    <div class="row">
                        <div class="col-sm-3">@st.Name - @st.MarksPercent%</div>
                        <div class="col-sm-3">@st.Email</div>
                        <div class="col-sm-3">@st.Class</div>
                        <div class="col-sm-3">@st.Address </div>
                    </div>
                }
            }
            <div class="clearfix"></div><br />
        </div>
    </div>
</div>
